{
  "manifest_version": 3,
  "name": "Bot Remover",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "client.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
